{
    "name": "hungnm1518/laravel-paginateroute",
    "description": "Laravel outer extension to easily use laravel's paginator without the query string",
    "keywords": [
        "laravel-paginateroute",
        "laravel",
        "pagination",
        "routing"
    ],
    "homepage": "https://github.com/hungnm1518/laravel-paginateroute",
    "license": "MIT",
    "authors": [
        {
            "name": "HungNM",
            "email": "hungnm144@gmail.com",
            "homepage": "https://github.com/hungnm1518/",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "^5.0",
        "illuminate/contracts": "~5.4.0",
        "illuminate/routing": "~5.4.0",
        "illuminate/translation": "~5.4.0",
        "orchestra/testbench": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "HungNM\\PaginateRoute\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HungNM\\PaginateRoute\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "HungNM\\PaginateRoute\\PaginateRouteServiceProvider"
            ],
            "aliases": {
                "PaginateRoute": "HungNM\\PaginateRoute\\PaginateRouteFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
